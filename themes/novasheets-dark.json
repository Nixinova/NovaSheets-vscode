{
	"name": "NovaSheets Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#1e1e1e",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Variable Declarator",
			"scope": [
				"novasheets.variable.declarator"
			],
			"settings": {
				"foreground": "#d089ff"
			}
		},
		{
			"name": "Variable Substitution Characters",
			"scope": [
				"novasheets.variable.substution"
			],
			"settings": {
				"foreground": "#4f5bff"
			}
		},
		{
			"name": "Variable Names",
			"scope": [
				"novasheets.variable.name"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff89a3"
			}
		},
		{
			"name": "Seperator",
			"scope": [
				"novasheets.seperator"
			],
			"settings": {
				"foreground": "#483dc4"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation"
			],
			"settings": {
				"foreground": "#dbdbd9"
			}
		},
		{
			"name": "Invalid Content",
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#ff5353"
			}
		},
		{
			"name": "Elements",
			"scope": [
				"css.element"
			],
			"settings": {
				"foreground": "#f7c872"
			}
		},
		{
			"name": "Property Names",
			"scope": [
				"css.property.name"
			],
			"settings": {
				"foreground": "#ce916a"
			}
		},
		{
			"name": "Property Value",
			"scope": [
				"css.property.value"
			],
			"settings": {
				"foreground": "#5ed9ff"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"css.function"
			],
			"settings": {
				"foreground": "#9f89ff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"css.string"
			],
			"settings": {
				"foreground": "#f5f28c"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"css.number"
			],
			"settings": {
				"foreground": "#4fffbc"
			}
		},
		{
			"name": "Escape Prefixes",
			"scope": [
				"css.escape"
			],
			"settings": {
				"foreground": "#d489ff"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"css.color.hash"
			],
			"settings": {
				"foreground": "#8af2d6"
			}
		},
		{
			"name": "Multiline Comment",
			"scope": [
				"css.comment"
			],
			"settings": {
				"foreground": "#337a33"
			}
		},
		{
			"name": "Inline Comment",
			"scope": [
				"novasheets.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#337a33"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		}
	]
}